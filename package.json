{
  "name": "@safeh2o/react-csv-importer",
  "license": "MIT",
  "version": "1.1.0",
  "description": "React CSV import widget with user-customizable mapping",
  "keywords": [
    "react",
    "csv",
    "upload",
    "parser",
    "import",
    "preview",
    "raw preview",
    "TextDecoder",
    "papa parse",
    "papaparse"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safeh2o/react-csv-importer"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "webpack --mode production && dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "lint": "eslint --max-warnings=0 --ext ts --ext tsx src",
    "lint-fix": "eslint --max-warnings=0 --ext ts --ext tsx src --fix",
    "stylelint": "stylelint \"src/**/*.scss\"",
    "stylelint-fix": "stylelint \"src/**/*.scss\" --fix",
    "test": "cross-env TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha --require ts-node/register --timeout 30000 test/**/*.test.ts",
    "prepublishOnly": "yarn build"
  },
  "author": "Nick Matantsev <nick.matantsev@beamworks.io>",
  "lint-staged": {
    "src/**/*.{ts,tsx}": "eslint --max-warnings=0",
    "src/**/*.scss": "stylelint",
    "test/**/*.{js,ts}": "eslint --max-warnings=0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@types/chai": "^4.2.14",
    "@types/chromedriver": "^81.0.2",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.18.2",
    "@types/papaparse": "^5.2.2",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/selenium-webdriver": "^4.0.11",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "babel-loader": "^9.1.3",
    "chai": "^4.2.0",
    "chromedriver": "^118.0.0",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.0.1",
    "dts-bundle-generator": "^8.0.1",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.1.0",
    "expose-loader": "^4.1.0",
    "file-loader": "^6.1.0",
    "lint-staged": "^14.0.1",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "prettier": "^2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.26.10",
    "sass-loader": "^13.3.2",
    "selenium-webdriver": "^4.0.0-alpha.8",
    "style-loader": "^3.3.3",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "stylelint-scss": "^5.2.1",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "dependencies": {
    "@use-gesture/react": "^10.2.11",
    "papaparse": "^5.3.0",
    "react-dropzone": "^14.3.5"
  },
  "packageManager": "yarn@4.6.0"
}
